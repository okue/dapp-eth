#! /usr/bin/env sh
geth --networkid "1234"\
     --nodiscover --datadir eth_private\
     --unlock 0,1\
     --password eth_private/passwd.txt\
     --rpc --rpcaddr "localhost"\
     --rpcport "8545"\
     --rpccorsdomain "*"\
     --mine --minerthreads "1"\
     console 2>> eth_private/geth_err.log
